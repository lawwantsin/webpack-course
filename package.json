{
  "name": "webpack-course",
  "version": "1.0.0",
  "description": "Webpack: Beyond the Basics Course",
  "repository": "https://github.com/lawwantsin/webpack-course.git",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "babel-cli": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-plugin-transform-runtime": "*",
    "babel-polyfill": "*",
    "babel-preset-env": "*",
    "babel-preset-es2015": "*",
    "css-loader": "*",
    "cssnano": "*",
    "ejs-loader": "*",
    "express": "*",
    "extract-loader": "*",
    "extract-text-webpack-plugin": "*",
    "file-loader": "*",
    "html-loader": "*",
    "html-webpack-plugin": "*",
    "optimize-css-assets-webpack-plugin": "*",
    "react": "*",
    "style-loader": "*",
    "url-loader": "*",
    "webpack": "*",
    "webpack-dev-server": "*",
    "webpack-hot-middleware": "*",
    "webpack-mild-compile": "*"
  },
  "scripts": {
    "start": "webpack-dev-server --config=config/webpack.dev.js",
    "build":
      "webpack --config=config/webpack.prod.js --env.NODE_ENV=production ",
    "build:dev": "webpack --config=config/webpack.dev.js",
    "dev": "nodemon --inspect src/server/main.js",
    "prod": "NODE_ENV=production node src/server/main.js"
  }
}
